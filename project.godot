; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="awesome calculator"
config/description="very useful"
config/version="1.0"
run/main_scene="res://Scenes/main_window.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/show_image=false
boot_splash/image="res://assets/logo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/icon.png"

[autoload]

global="*res://Scripts/globalvars.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=480
window/size/resizable=false
window/size/transparent=true
window/per_pixel_transparency/allowed=true

[gui]

theme/custom="res://theme/def theme.tres"
theme/custom_font="res://assets/fonts/Comic Sans MS.ttf"

[rendering]

environment/defaults/default_clear_color=Color(1, 1, 1, 1)
